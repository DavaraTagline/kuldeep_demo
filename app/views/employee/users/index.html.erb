<p id="notice"><%= notice %></p>
<%= flash_messages %>
<h1 class="text-center">Userdetails</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Gender</th>
      <th scope="col">State</th>
      <th scope="col">City</th>
      <th scope="col" colspan=3>Action</th>
    </tr>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= @user.id %></td>
        <td><%= @user.name %></td>
        <td><%= @user.email %></td>
        <td><%= @user.phone %></td>
        <td><%= @user.gender %></td>
        <td><%= @user.state.name %></td>
        <td><%= @user.city.name %></td>
        <td><%= link_to 'show',employee_user_path(@user)%></td>
        <td><%= link_to 'Edit',edit_employee_user_path(@user)%></td>
        <td><%= link_to 'Destroy',employee_user_path(@user), method: :delete, data: { confirm: 'Are you sure?'} %></td>
      </tr>
  </tbody>
</table>
