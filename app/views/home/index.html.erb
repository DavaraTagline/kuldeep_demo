<div class="container mt-4">
<div class="jumbotron">
  <h1 class="display-4">Welcome to Demo</h1>
  <p class="lead">Demo app</p>
  <hr class="my-4">
    <% unless user_signed_in? %>
      <%= link_to 'Sign up', new_user_registration_path,class:"btn btn-primary btn-lg" %>
      <%= link_to 'Log in', user_session_path,class:"btn btn-warning btn-lg" %>
      <%= link_to 'sign in with google', user_google_oauth2_omniauth_authorize_path, method: :post, class: "btn btn-info btn-lg" %>
      <%= link_to 'sign in with Facebook', user_facebook_omniauth_authorize_path, method: :post, class: "btn btn-dark btn-lg" %>
    <% end %>
</div>
</div>