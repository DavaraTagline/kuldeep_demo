<%= form_with model: @accountdetail, url: url do |f| %>

  <%#= f.hidden_field :user_id, value: params[:user_id] %>
  <%= f.hidden_field :user_id, value: @accountdetail.user_id %>

  <div class="form-group row">
    <%= f.label :branch_name,  class:"col-sm-2 col-form-label" %><br />
    <div class="col-sm-8">
    <%= f.text_field :branch_name, autofocus: true, class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :account_number, class:"col-sm-2 col-form-label" %><br />
    <div class="col-sm-8">
    <%= f.number_field :account_number, autofocus: true,class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :account_name, class:"col-sm-2 col-form-label" %><br />
    <div class="col-sm-8">
    <%= f.text_field :account_name, autofocus: true,class: "form-control" %>
    </div>
  </div>

  <div class="form-group row">
    <%= f.label :image, class:"col-sm-2 col-form-label" %><br />
    <div class="col-sm-8">
    <%= f.file_field :image %>
    </div>
  </div>
  
  <div class="actions">
    <%= f.submit @accountdetail.new_record? ? "Add" : "Update" ,class:"btn btn-primary btn-sm" %><br/>
  </div>
<% end %>